#include <iostream>

using namespace std;

int ankyy(int i)
{
    int sum=0;
    int r;
    while(i>0)
    {
        r=i%10;
        sum=sum+r;
        i=i/10;
    }
    return sum;
};


int main()
{
    //ankyy(345);
    cout<<ankyy(2564);
    return 0;
}
